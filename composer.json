  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "Nick Davis",
      "email": "inbox@nickdavis.co"
    }
  ],
  "require": {
    "php": ">=7.0",
    "johnbillion/extended-cpts": "^4.2"
  },
